# Supabase Configuration
# IMPORTANT: Get these values from your Supabase Dashboard > Settings > API
# Make sure to enable "Email Sign-ups" in Authentication > Settings
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Meta Ads API
VITE_META_APP_ID=your_meta_app_id
VITE_META_APP_SECRET=your_meta_app_secret

# TikTok Ads API
VITE_TIKTOK_APP_ID=your_tiktok_app_id
VITE_TIKTOK_APP_SECRET=your_tiktok_app_secret

# Google Ads API
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret
VITE_GOOGLE_DEVELOPER_TOKEN=your_google_developer_token

# OpenAI API
# IMPORTANTE: Não commite sua chave real! Use esta variável ou configure via interface da aplicação
# Obtenha sua chave em: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# OAuth Redirect URLs
# Configure de acordo com seu ambiente:
# Desenvolvimento: http://localhost:5173/oauth-callback
# Produção: https://seu-dominio.com/oauth-callback
VITE_OAUTH_REDIRECT_URL=http://localhost:5173/oauth-callback

# Encryption Key for API Credentials
# IMPORTANT: Generate a secure random key for production
# You can use: openssl rand -base64 32
VITE_ENCRYPTION_KEY=change-this-to-a-secure-random-key-in-production